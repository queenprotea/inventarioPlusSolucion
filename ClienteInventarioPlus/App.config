<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IUsuarioService" />
                <binding name="BasicHttpBinding_IProveedorService"/>
                <binding name="BasicHttpBinding_IReservaService"/>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint
                name="UsuarioServiceEndpoint"
                address="http://192.168.1.68:8080/UsuarioServicio"
                binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IUsuarioService"
                contract="BibliotecaClasesNetframework.Contratos.IUsuarioService"
            />
            <endpoint
                name="ProveedorServiceEndpoint"
                address="http://192.168.1.68:8080/ProveedorServicio"
                binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IProveedorService"
                contract="BibliotecaClasesNetframework.Contratos.IProveedorService"
            />
            <endpoint
                name="ReservaServiceEndpoint"
                address="http://192.168.1.68:8080/ReservaServicio"
                binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IReservaService"
                contract="BibliotecaClasesNetframework.Contratos.IReservaService"
            />
        </client>
    </system.serviceModel>
</configuration>